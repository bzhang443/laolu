<p id="notice"><%= notice %></p>

<h1>Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Token</th>
      <th>Customer</th>
      <th>Device</th>
      <th>Ip</th>
      <th>Geo</th>
      <th>Expire at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.token %></td>
        <td><%= session.customer_id %></td>
        <td><%= session.device_id %></td>
        <td><%= session.ip_address %></td>
        <td><%= session.geo %></td>
        <td><%= session.expire_at %></td>
        <td><%= link_to 'Show', session %></td>
        <td><%= link_to 'Edit', edit_session_path(session) %></td>
        <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session', new_session_path %>
