<%= form_with(model: session, local: true) do |form| %>
  <% if session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(session.errors.count, "error") %> prohibited this session from being saved:</h2>

      <ul>
      <% session.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :token %>
    <%= form.text_field :token %>
  </div>

  <div class="field">
    <%= form.label :customer_id %>
    <%= form.number_field :customer_id %>
  </div>

  <div class="field">
    <%= form.label :device_id %>
    <%= form.number_field :device_id %>
  </div>

  <div class="field">
    <%= form.label :ip %>
    <%= form.text_field :ip_address %>
  </div>

  <div class="field">
    <%= form.label :geo %>
    <%= form.text_field :geo %>
  </div>

  <div class="field">
    <%= form.label :expire_at %>
    <%= form.datetime_select :expire_at %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
